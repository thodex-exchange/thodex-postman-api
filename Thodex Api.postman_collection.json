					"var CryptoJS = require(\"crypto-js\");",
					"const paramsString = request.url.split('?')[1];",
					"const eachParamArray = paramsString.split('&');",
					"let params = {};",
					"eachParamArray.forEach((param) => {",
					"    const key = param.split('=')[0];",
					"    const value = param.split('=')[1];   ",
					"    Object.assign(params, {[key]: value});",
					"});",
					"const d = new Date();",
					"const tonce = Math.round(d.getTime()/1000);",
					"pm.environment.set('tonce', tonce);",
					"Object.assign(params, {[\"apikey\"]: pm.variables.get('apikey')});",
					"Object.assign(params, {[\"tonce\"]:  tonce});",
					"var sigString = http_build_query(params)+\"&secret=\"+pm.variables.get('apisecret');",
					"console.log(sigString);",
					"var token = CryptoJS.SHA256(sigString);",
					"pm.request.headers.upsert({key: 'Authorization', value: token.toString(CryptoJS.enc.Hex) });",
					"",
					"",
					"function http_build_query(jsonObj) {",
					"  const keys = Object.keys(jsonObj).sort();",
					"  const values = keys.map(key => jsonObj[key]);",
					"  return keys",
					"    .map((key, index) => {",
					"      return `${key}=${values[index]}`;",
					"    })",
					"    .join(\"&\");",
					"};",
					"",
					""
				]
			}
